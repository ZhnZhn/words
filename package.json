{
  "name": "words",
  "version": "0.3.0",
  "description": "Web app Words",
  "main": "index.js",
  "scripts": {
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "lint": "eslint src",
    "css:min": "gulp",
    "build:dll": "webpack --color --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --color --progress --config webpack.dll-jsx.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:bundle": "gulp jspm"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/words.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "del": "^5.1.0",
    "eslint": "^7.23.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^25.1.0",
    "jspm": "^0.16.55",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  },
  "jspm": {
    "dependencies": {
      "@babel/runtime": "npm:@babel/runtime@^7.12.1",
      "interactjs": "npm:interactjs@1.3.4",
      "localforage": "npm:localforage@1.5.7",
      "react": "npm:react@^17.0.1",
      "react-dom": "npm:react-dom@^17.0.1",
      "reflux-core": "npm:reflux-core@^1.0.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "interactjs": "^1.3.4",
    "localforage": "^1.5.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reflux-core": "^1.0.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/js/flux/stores/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/js/flux/stores/"
    ]
  }
}
