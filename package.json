{
  "name": "words",
  "version": "0.3.0",
  "description": "Web app Words",
  "main": "index.js",
  "scripts": {
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "lint": "eslint src",
    "css:min": "gulp",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:bundle": "gulp jspm"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/words.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "del": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jspm": "^0.16.55",
    "terser-webpack-plugin": "^2.2.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "jspm": {
    "dependencies": {
      "@babel/runtime": "npm:@babel/runtime@^7.7.6",
      "interactjs": "npm:interactjs@1.3.4",
      "localforage": "npm:localforage@^1.5.6",
      "react": "npm:react@^16.12.0",
      "react-dom": "npm:react-dom@^16.12.0",
      "reflux": "npm:reflux@5.0.4"
    },
    "devDependencies": {}
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "interactjs": "^1.3.4",
    "localforage": "^1.5.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reflux": "^5.0.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/js/flux/stores/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/js/flux/stores/"
    ]
  }
}
